MODULE modify_screen OUTPUT.
  LOOP AT SCREEN.
    IF screen-name EQ 'ZOBSV_CONFIG-OBSSK'.
      screen-invisible = 1.
    ENDIF.
    MODIFY SCREEN.
  ENDLOOP.
ENDMODULE.
